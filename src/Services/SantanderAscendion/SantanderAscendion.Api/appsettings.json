{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information"
  },
  "AllowedHosts": "*",
  "HackerNews": {
    "BaseUrl": "https://hacker-news.firebaseio.com/v0/",
    "BestStoriesEndpoint": "beststories.json",
    "ItemEndpoint": "item",
    "TimeoutSeconds": 120,
    "CacheExpirationSeconds": 300
  }
}
